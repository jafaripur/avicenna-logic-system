{
    "name": "jafaripur/avicenna-logic-system",
    "type" : "library",
    "version": "1.0.0",
    "description": "Tool for analyzing logical proofs and generating truth tables with propositional logic support.",
    "keywords": ["logic", "natural deduction", "proposition logic", "truth-table"],
    "license": "MIT",
    "minimum-stability" : "dev",
    "prefer-stable": true,
    "readme": "README.md",
    "authors": [
        {
            "name": "Araz J",
            "email": "mjafaripur@yahoo.com",
            "homepage": "https://jafaripur.com",
            "role": "Developer"
        }
    ],
    "support": {
        "email": "mjafaripur@yahoo.com"
    },
    "require": {
        "php": "^8.3"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.69.0",
        "overtrue/phplint": "^9.5.6",
        "phpunit/phpunit": "^12.0",
        "symfony/console": "^7.2"
    },
    "autoload": {
        "psr-4": { "Avicenna\\": "src/" }
    },
    "config": {
        "optimize-autoloader": true,
        "classmap-authoritative": true,
        "sort-packages": true,
        "preferred-install": {
            "*": "dist"
        }
    }
}
